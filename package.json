{
  "name": "online-reader",
  "version": "1.0.0",
  "description": "online book reader ",
  "main": "sever/index.js",
  "scripts": {
    "server": "cd server && npm run dev",
    "client": "cd client && npm start",
    "start": "NODE_ENV=production node server/index.js",
    "dev": "nodemon server/index.js",
    "kill": "fuser -k 5000/tcp",
    "kill-run": "npm run kill && npm run server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mFlaifel/online-reader.git"
  },
  "keywords": [
    "['react'",
    "'node.js'",
    "'mongoDB'",
    "'express'",
    "'epub']"
  ],
  "author": "mFlaifel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mFlaifel/online-reader/issues"
  },
  "homepage": "https://github.com/mFlaifel/online-reader#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.13",
    "nodemon": "^2.0.3"
  }
}
